{
  "buildCommand": "echo 'Static build'",
  "outputDirectory": "frontend",
  "installCommand": "echo 'No install needed'",
  "env": {
    "AUTH_SERVICE": "https://auth-service-v5sb.onrender.com",
    "APP_SERVICE": "https://app-service-kupj.onrender.com"
  },
  "routes": [
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/registro",
      "dest": "/registro.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
